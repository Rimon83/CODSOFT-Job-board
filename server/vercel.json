{
  "version": 2,
  "builds": [
    { "src": "/server/index.js", "use": "@vercel/node" },
    { "src": "server/build/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "server/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "server/(.*)",
      "dest": "/server/build/$1"
    }
  ]
}
